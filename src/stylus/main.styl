
.header
  background-color:#333;

  h2
    margin-top: 10px;

  .menu
    text-align:right;

    h5
      color:#999
      display:inline-block;
      padding: 10px 10px 10px;

  .title
    color:#cc3333;

.red
  color: #cc3333;
  
.title
  border-bottom: 1px solid #000000;

.text-align
  text-align: right

.wrapper
  background-color:#F5F5F5;
  border-radius: 10px;
  margin-top: 20px;
  padding: 20px 50px 20px;

  .start
    background-color: #CC3333;
    height: 95px;

    .rotate
      -moz-transform: rotate(-45deg);
      -webkit-transform: rotate(-45deg);
      -o-transform: rotate(-45deg);
      -ms-transform: rotate(-45deg);
      transform: rotate(-45deg);

  h3
    margin-top: 0;

  h4
    color:#666;

  .animation-area
    background-color:white;
    border-radius: 10px;
    height: 475px;
    overflow: hidden;

    .img-container
      background-image: url(../assets/img/avatar.png);
      background-repeat: no-repeat;
      background-position: 0px -30px;
      height: 350px;
      margin: 30px auto;
      width: 200px;

    .word-area
      padding: 0 25px;

      .col-sm-1
        padding-left: 5px;
        padding-right: 5px;

      .medium-padding
        padding: 0 10px;

      .word
        background-color:#F5F5F5;
        border-radius: 5px;
        border: 1px solid #999;
        height: 20px;

      .padding-top
        padding-top: 10px;





